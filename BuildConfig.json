kind: "BuildConfig"
apiVersion: "v1"
metadata:
  name: "dependency-viewer" 
spec:
 source: 
    type: "Git"
    git:
      uri: "git://github.com/jasinner/jboss-dependency-viewer.git"
  strategy: 
    type: "Source"
    sourceStrategy:
      from:
        kind: "ImageStreamTag"
        name: "php-55-centos7:latest"
  output: 
    to:
      kind: "ImageStreamTag"
      name: "dependency-viewer:latest"

